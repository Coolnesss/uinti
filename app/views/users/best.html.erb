<div class="col-md-6 col-md-offset-2">
  <h1>Listing ranked users</h1>
  <ul class="mdl-list">
    <% @users.each do |user| %>
      <li class="mdl-list__item">
        <span class="mdl-list__item-primary-content">
          <%= image_tag user.image_of_rank, style: "margin-right: 3em" %>
          <%= link_to user, user %>
        </span>
      </li>
    <% end %>
  </ul>
</div>
